<div class="sheet-combat">
    <div class="combat-main">
        <div class="mainStat" id="HP">Hitpoints: <span>${hitPoints}</span></div>
        <div class="mainStat" id="AC">Armor Class: <span></span></div>
        <div class="mainStat" id="T-AC">Touch Armor Class: <span></span></div>
        <div class="mainStat" id="FF-AC">FF Armor Class: <span></span></div>
        <div class="mainStat" id="initiative">Initiative: <span></span></div>
        <div class="mainStat" id="BAB">Base attack bonus: <span>${BAB}</span></div>
        <div class="mainStat" id="speed">Speed: <span></span> feat</div>
        <div class="mainStat" id="spellRes">Spell Resistance: <span></span></div>
        <div class="mainStat" id="grapple">Grapple: <span></span></div>
    </div>
    <div class="combat-saves"></div>
        <div class="saveStat" id="fort">Fortitude save: <span>${saveFort}</span></div>
        <div class="saveStat" id="reflex">Reflex save: <span>${saveReflex}</span></div>
        <div class="saveStat" id="will">Will save: <span>${saveWill}</span></div>
    </div>
    <div class="combat-gear"></div>
</div>